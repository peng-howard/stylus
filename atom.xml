      <category term="轮廓图" scheme="http://peng-howard.github.io/stylus/tags/%E8%BD%AE%E5%BB%93%E5%9B%BE/"/>
    
      <category term="平行坐标图" scheme="http://peng-howard.github.io/stylus/tags/%E5%B9%B3%E8%A1%8C%E5%9D%90%E6%A0%87%E5%9B%BE/"/>
    
